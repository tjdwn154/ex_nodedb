  <body>
    <h4 class="text-center">SJ Dev</h4>
    <%- include('nav.html') %>
    <h3 class="text-center ml-2 my-3">Wirte Main</h3>
    <div class="container mt-3">
    <form action="/add" method="POST">
      <div class="form-group">
        <label>제목</label>
        <input type="text" class="form-control" name="title">
      </div>
      <div class="form-group">
        <label>등록일</label>
        <input type="text" class="form-control" name="date" placeholder="YYYY-MM-DD">
      </div>
      <div class="form-group">
        <label>내용</label>
        <input type="text" class="form-control" name="contents">
      </div>
      <div class="form-group">
        <label>시각</label>
        <input type="text" class="form-control" name="contentimg">
      </div>
      <button type="submit" class="btn btn- bg-success text-white">등록</button>
    </form>
  </div>

    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
